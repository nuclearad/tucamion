<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">
      Tipos <small>/ Servicio</small>
    </h1>
    <%= render 'admin/includes/top' %>
  </div>
</div>

<!-- /.row -->
<% if flash[:notice] %>

<%= render partial: 'shared/flashNoticeMessage' %>


<% end %>

<!-- /.row -->

<div class="row">
    <div class="col-md-3">
        <p>
            <%= link_to 'Nuevo tipo', new_admin_type_service_path,:method=>'get', :class=>'btn  btn-primary' %>
        </p>
    </div>
    <div class="col-md-3 col-md-offset-6">
        <%= render partial: 'includes/admin_search', locals: {object: @search, search_path: request.path, query_search_field: @query_search_field} %>
    </div>
</div>

<div class="row">
  <div class="col-lg-12">
    <%= render partial: 'shared/tableResult', locals:{objects:@types_filter, object_controller_name: controller_name} %>
  </div>
</div>
<!-- /.row -->